{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@api/*": ["./api/*"],   
      "@components/*":["./components/*"],
      "@models/*":["./models/*"],
      "@hook/*":["./hook/*"],
      "@store/*":["./store/*"],
      "@app/*":["./app/*"],  
      "@util/*":["./util/*"]    
      
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx", "components/ui/authenticateMenu.jsx"
, "components/ui/admin-account.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
